{
  "name": "@qimao/quantum-ui",
  "version": "1.0.0",
  "description": "",
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "vite build"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/lib/index.js"
    },
    "./es": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.js"
    },
    "./lib": {
      "types": "./dist/index.d.ts",
      "require": "./dist/lib/index.js"
    },
    "./es/*": {
      "types": "./dist/src/*.d.ts",
      "import": "./dist/es/*.js"
    },
    "./lib/*": {
      "types": "./dist/src/*.d.ts",
      "require": "./dist/lib/*.js"
    },
    "./*": "./*"
  },
  "keywords": [],
  "author": "Evan Wu",
  "license": "ISC",
  "dependencies": {
    "@qimao/quantum-utils": "workspace:*",
    "@qimao/quantum-core": "workspace:*",
    "@q-front-npm/vue3-antd-pc-ui": "~1.2.10-beta.4",
    "@qimao/quantum-schemas": "workspace:*"
  },
  "peerDependencies": {
    "vue": "~3.3.4"
  },    
  "devDependencies": {
    "@q-front-npm-configs/vite": "~1.2.5-beta.1",
    "@q-front-npm/types": "~1.2.1",
    "sass": "^1.63.3",
    "@ant-design/icons-vue": "^6.1.0",
    "vue-types": "^3.0.2",
    "csstype": "~3.1.2",
    "prettier": "~3.1.0",
    "vue": "~3.3.4",
    "vite-plugin-dts": "3.6.4"
  }
}
