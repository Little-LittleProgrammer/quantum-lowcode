{
  "name": "@quantum-lowcode/ui",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "vite build"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/lib/index.js"
    },
    "./es": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.js"
    },
    "./lib": {
      "types": "./dist/index.d.ts",
      "require": "./dist/lib/index.js"
    },
    "./es/*": {
      "types": "./dist/src/*.d.ts",
      "import": "./dist/es/*.js"
    },
    "./lib/*": {
      "types": "./dist/src/*.d.ts",
      "require": "./dist/lib/*.js"
    },
    "./*": "./*"
  },
  "keywords": [],
  "author": "Evan Wu",
  "license": "ISC",
  "dependencies": {
    "@quantum-lowcode/utils": "workspace:*",
    "@quantum-lowcode/core": "workspace:*",
    "@quantum-design/vue3-antd-pc-ui": "catalog:",
    "@quantum-design/styles": "catalog:",
    "@quantum-lowcode/schemas": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },    
  "devDependencies": {
    "@quantum-design-configs/vite": "catalog:",
    "@quantum-design/types": "catalog:",
    "sass": "catalog:",
    "@ant-design/icons-vue": "catalog:",
    "vue-types": "catalog:",
    "csstype": "catalog:",
    "vue": "catalog:",
    "vite-plugin-dts": "catalog:"
  }
}
